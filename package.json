{
  "name": "resin-lint",
  "version": "1.1.4",
  "description": "Resin.io coffeelint wrapper",
  "bin": "./bin/resin-lint",
  "main": "./build/resin-lint.js",
  "homepage": "https://github.com/resin-io/resin-lint",
  "repository": {
    "type": "git",
    "url": "git@github.com:resin-io/resin-lint"
  },
  "scripts": {
    "pretest": "./node_modules/.bin/coffee -o build -c lib",
    "test": "./bin/resin-lint test/",
    "prepublish": "npm run lint lib/ && ./node_modules/.bin/coffee -o build -c lib",
    "lint": "./bin/resin-lint"
  },
  "keywords": [
    "resin",
    "coffeescript",
    "coffeelint"
  ],
  "author": "Kostas Lekkas <kostas@resin.io",
  "license": "Apache 2.0",
  "dependencies": {
    "coffeelint": "^1.15.0",
    "minimist": "^1.2.0",
    "resolve": "^1.1.6"
  },
  "devDependencies": {
    "coffee-script": "^1.10.0"
  }
}
